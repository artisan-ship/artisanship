<% if(error && error.length > 0){ %>
  <div class="ui container" style="padding:  30px 0;">
<div class="ui warning message">
  <i class="close icon"></i>
  <div class="header">
    <%= error %>
  </div>

</div>
</div>

<% } %>
<% if(success && success.length > 0){ %>
  <div class="ui container" style="padding: 30px 0;">
<div class="ui positive message">
  <i class="close icon"></i>
  <div class="header">
    <%= success %>
  </div>
 
</div>
</div>
<% } %>