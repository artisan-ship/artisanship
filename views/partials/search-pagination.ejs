<div class="ui center aligned container" style="padding-bottom:20px;">
    <% if (pages && pages > 0) { %>
    <div class="ui pagination menu">
        <% if (current == 1) { %>
            <a class="disabled item">First</a>
            <% } else { %>
                <a href="/admin/<%= currentUser.id %>/search" class="item">First</a>
            <% } %>
     
            <% if (current == 1) { %>
                <a class="disabled item"><i class="angle  left icon"></i></a>
            <% } else { %>
                <a class="item" href="/admin/<%= currentUser.id %>/search?page=<%= Number(current) - 1 %> "><i class="angle  left icon"></i></a>
        
            <% } %>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
         


        <% if (i !== 1) { %>
            <a class="item disabled">...</a>
            <% } %>

        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
            <% if (i == current) { %>
            <a class="item active"><%= i %></a>
            <% } else { %>
            <a class ="item" href="/admin/<%= currentUser.id %>/search?page=<%= i %>"><%= i %></a>
            <% } %>
            <% } %>


            <% if (current == pages) { %>
                <a class="disabled item"><i class="angle right  icon"></i></a>
                <% } else { %>
                    <a class="item" href="/admin/<%= currentUser.id %>/search?page=<%= Number(current) + 1 %> "><i class="angle right icon"></i></a>

                <% } %>
         
                <% if (current == pages) { %>
                    <a class="disabled item">Last</a>
                <% } else { %>
                <li><a href="/admin/<%= currentUser.id %>/search?page=<%= pages %>">Last</a></li>
                <% } %>
      </div>
      <% } %>
</div>